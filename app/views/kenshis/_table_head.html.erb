<thead>
  <tr>
    <th>Title</th>
    <th>
      <%= Kenshi.human_attribute_name :name %>
    </th>
    <% if user_signed_in? %>
      <th>
        <%= Kenshi.human_attribute_name :email %>
      </th>
      <th>
        <%= Kenshi.human_attribute_name :dob %>
      </th>
    <% end %>
    <th>
      <%= Kenshi.human_attribute_name :club %>
    </th>
    <th>
      <%= Kenshi.human_attribute_name :grade %>
    </th>
    <th>
      <%= Kenshi.human_attribute_name :categories %>
    </th>
    <% if user_signed_in? %>
      <th>
        <%= t("kenshi.registered_by") %>
      </th>
      <th></th>
    <% end %>
  </tr>
</thead>
